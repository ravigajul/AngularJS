{
	"name": "AngularJS",
	"version": "1.0.0",
	"description": "Jasmine--JavaScript Framework\r Mocha,Chai are other java script frameworks",
	"dependencies": {
		"protractor": "5.4.2",
		"jasmine-data-provider": "2.2.0"
	},
	"scripts": {
		"webdriver:update": "./node_modules/.bin/webdriver-manager update",
		"webdriver:start": "./node_modules/.bin/webdriver-manager start",
		"protractor": "./node_modules/.bin/protractor ./ConfigFiles/config.js",
		"SmokeTest": "./node_modules/.bin/protractor ./ConfigFiles/config.js --suite smoke ",
		"RegressionTest": "./node_modules/.bin/protractor ./ConfigFiles/config.js --suite Regression",
		"start": "npm run webdriver:update && npm run webdriver:start"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ravigajul/AngularJS.git"
	}
}
